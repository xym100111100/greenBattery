    font-weight: bold;
    font-size: 14pt;
    color: #555;
    text-shadow: 1rpx 1rpx #ccc;
}

.small {
    font-weight: normal;
}

.kg {
    width: 250rpx;
    text-align: right;
}

.type {
    font-size: 10pt;
    padding: 0 10rpx;
