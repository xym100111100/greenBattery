    color: #999;
    text-align: right;
}

.unit {
    color: #aaa;
    font-size: 9pt;
    padding-left: 6rpx;
}

.weui-cell__ft {
    font-size: 10pt;
}

.green {
    color: green;
}

.red {
    color: rgb(151,49,49);
}

