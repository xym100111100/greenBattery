.date-hd {
    color: #777;
    font-size: 11pt;
    white-space: nowrap;
    flex: 1 1 auto;
}

.date {
    font-family: Arial,Helvetica,sans-serif;
    font-size: 11pt;
    padding: 0 20rpx;
    white-space: nowrap;
}

.date-split {
    color: #777;
    font-size: 12pt;
}

.btn-log {
    font-size: 10pt!important;
    font-weight: normal;
    white-space: nowrap;
}

.row {
    display: flex;
    justify-content: flex-start;
    align-items: baseline;
}

.left {
    justify-content: flex-start;
}

.action {
    font-size: 11pt;
    color: #666;
    white-space: nowrap;
}

.number {
