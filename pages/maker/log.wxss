.date-hd {
    color: #777;
    font-size: 11pt;
    flex: 1 1 auto;
    white-space: nowrap;
}

.date {
    font-family: Arial,Helvetica,sans-serif;
    font-size: 11pt;
    padding: 0 20rpx;
    white-space: nowrap;
}

.date-split {
    color: #777;
    font-size: 12pt;
}

.btn-log {
    font-size: 10pt!important;
    font-weight: normal;
    white-space: nowrap;
}

scroll-view {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: stretch;
}

.row {
    flex: 1 1 auto;
    display: flex;
    justify-content: flex-end;
    align-items: baseline;
}

.left {
    justify-content: flex-start;
}

.action {
    font-size: 11pt;
    color: #aaa;
    white-space: nowrap;
}

.number {
    font-family: Arial;
    font-weight: bold;
    font-size: 14pt;
    color: #555;
    text-shadow: 1rpx 1rpx #ccc;
}

.small {
    font-weight: normal;
}

.kg {
    text-align: right;
}

.type {
    font-size: 10pt;
    padding: 0 10rpx;
    width: 70rpx;
    color: #999;
    text-align: right;
}

.unit {
    color: #aaa;
    font-size: 9pt;
    padding-left: 6rpx;
}

.weui-cell__ft {
    font-size: 10pt;
    white-space: nowrap;
    margin-left: 20rpx;
    color: #ddd!important;
}